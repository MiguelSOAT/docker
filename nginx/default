server {
	listen 80 default_server;
	listen [::]:80 default_server;


	root /var/www/html;

	index index.html index.htm index.nginx-debian.html;

	server_name _;
	

	location /api {
      rewrite /api/(.*) /$1 break;
      proxy_pass http://cloud-core:8081;
  	}

	location / {
		# serve static frontend first
		alias /var/www/html;
		try_files $uri $uri/ =404;
		// proxy_pass http://cloud-core:8081;
	}
}